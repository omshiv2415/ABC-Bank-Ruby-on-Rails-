	<th colspan="1">Transaction Date</th>
	<th colspan="1">Account Number</th>
</tr>
</thead>
<tbody>
	<% obj.each do |transaction|  %>
      <tr>

        <td><%= transaction.transType %></td>
        <td><%= transaction.method %></td>
