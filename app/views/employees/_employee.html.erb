<div class="page-header">
    <h3>ABC Bank's Employees</h3>
   <div class="contextual">
  </div>
</div>
<table class="table table-striped table-bordered table-condensed">
<thead>
  <thead>
    <tr>
      <th colspan="1">Fullname</th>
      <th colspan="1">Address</th>
      <th colspan="1">Phone</th>
      <th colspan="1">Employee Role</th>
      <th colspan="1">E-mail</th>
      <th colspan="1">Branch</th>
			<th colspan="1">Update </th>

    </tr>
  </thead>

  <tbody>
    <% obj.each do |employee| %>
      <tr>
        <td><%= employee.fullname %></td>
        <td><%= employee.address %></td>
        <td><%= employee.phone %></td>
        <td><%= employee.employeeRole %></td>
        <td><%= employee.email %></td>
        <td><%= employee.branch %></td>
				<td><%= link_to 'View', employee %>
        <%= link_to 'Edit', edit_employee_path(employee) %>
				<%= link_to 'Delete', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_employee_path %>
<div align="center">
<%= will_paginate %>
</div>